{
  "name": "alien-empires-bot",
  "version": "1.0.0",
  "description": "full-stack version of Space Empires: 4x alien empires management app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "engines": {
    "node": "8.11.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jon-Varner/alien-empires-bot.git"
  },
  "author": "Jon Varner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jon-Varner/alien-empires-bot/issues"
  },
  "homepage": "https://github.com/Jon-Varner/alien-empires-bot#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
